{
  "name": "cmd.js",
  "version": "0.0.2",
  "description": "A chainable utility toolkit for JavaScript.",
  "main": "cmd.js",
  "dependencies": {},
  "devDependencies": {
    "gulp": "^3.8.8",
    "gulp-cached": "^1.0.1",
    "gulp-concat": "^2.4.1",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.2.2",
    "gulp-jshint": "^1.8.5",
    "gulp-remember": "^0.2.1",
    "gulp-watch": "^1.1.0",
    "istanbul": "^0.3.5",
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.1.0"
  },
  "scripts": {
    "lint": "./node_modules/.bin/jshint ./cmd.js ./lib --reporter=node_modules/jshint-stylish/stylish.js",
    "checkStyle": "./node_modules/.bin/jscs ./cmd.js ./lib",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --check-leaks --recursive -R spec",
    "posttest": "./node_modules/.bin/istanbul check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NateFerrero/cmd.js.git"
  },
  "keywords": [
    "underscore",
    "lodash",
    "tool",
    "utility",
    "utilities",
    "command",
    "cmd",
    "commands",
    "script"
  ],
  "author": "Nate Ferrero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NateFerrero/cmd.js/issues"
  },
  "homepage": "https://github.com/NateFerrero/cmd.js"
}
